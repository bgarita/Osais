<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servLets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>servLets.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PreciosyExistencias</servlet-name>
        <servlet-class>servLets.PreciosyExistencias</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PreciosyE</servlet-name>
        <servlet-class>servLets.PreciosyE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaFNDNC</servlet-name>
        <servlet-class>servLets.ConsultaFNDNC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstadoCTA</servlet-name>
        <servlet-class>servLets.EstadoCTA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AntigSaldos</servlet-name>
        <servlet-class>servLets.AntigSaldos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstadoCXCx</servlet-name>
        <servlet-class>servLets.EstadoCXCx</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PreciosyExistencias</servlet-name>
        <url-pattern>/PreciosyExistencias</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PreciosyE</servlet-name>
        <url-pattern>/PreciosyE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaFNDNC</servlet-name>
        <url-pattern>/ConsultaFNDNC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstadoCTA</servlet-name>
        <url-pattern>/EstadoCTA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AntigSaldos</servlet-name>
        <url-pattern>/AntigSaldos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstadoCXCx</servlet-name>
        <url-pattern>/EstadoCXCx</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
